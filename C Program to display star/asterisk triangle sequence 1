#include<stdio.h> 
//#include <conio.h>

void main() {


//    clrscr();

    int i = 0; //for loop1
    
    int j = 0; //for loop2

    for(i = 0; i <= 10 ; i++) {
        
        for(j = 0; j <= i; j++) {
            
            printf("* ");
        }
        
        printf("\n");
        
    }

//	getch();


}

